<body id="login-page">
<div id="login-div">
<h1 id="login-text">Log In</h1>
<div id="log-in">
<p id="alert"><%= alert %></p>
<%= form_tag sessions_path do %>
<div class ="login-form">
  <div class="field">
    <p>
    <%= label_tag :email %>
    </p>
    <%= text_field_tag :email, params[:email] %>
  </div>
  <div class="field">
    <p>
    <%= label_tag :password %>
    </p>
    <%= password_field_tag :password %>
  </div>
  <div id="login-btn"><%= submit_tag "Log In" %></div>
<% end %>
</div>
</div>
</div>
</body>